//time : O(nklog(nk)) 
cclass Solution {
    public int nthSuperUglyNumber(int n, int[] primes) {
        PriorityQueue<Long> pq = new PriorityQueue<>();
        pq.add(1L);
        while(n-->1){
            long x = pq.peek();
            while(!pq.isEmpty() && x==pq.peek())pq.poll();
            for(int ele : primes)pq.add(x*ele);
        }
        return (int)(long)pq.poll();
    }
}
